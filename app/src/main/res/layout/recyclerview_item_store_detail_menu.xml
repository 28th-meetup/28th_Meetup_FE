<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="15dp"
    android:layout_marginTop="13.7dp"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageview_menu"
        android:layout_width="171.25dp"
        android:layout_height="154.13dp"
        android:src="@drawable/food" />

    <TextView
        android:id="@+id/textview_menu_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="밑반찬 세트"
        android:textColor="#000000"
        android:textSize="18.27sp"
        android:fontFamily="@font/pretendard_semi_bold"
        android:layout_marginTop="9.13dp"/>

    <TextView
        android:id="@+id/textview_menu_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="00000원"
        android:textColor="#000000"
        android:fontFamily="@font/pretendard_semi_bold"
        android:textSize="16sp"/>


</LinearLayout>